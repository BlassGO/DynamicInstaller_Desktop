#Dynamic Installer Config file
#Function: convert

#Rules for modifying this file
#  > Version: 5.3-b2+
#    1. All numbers ​​with four decimal zeros (0.00001) must be written in scientific notation (1e-05)
#    2. A maximum of sixth decimals is allowed
#--------------------------

#Format:
#[[GROUP NAME]: "[UNIT]" "[UNIT]" "[...]" ]
#
#[NUMBER][UNIT] = [NUMBER][UNIT]
#[...][...]     = [...][...]
#
#[[GROUP NAME] end]

[storage: "b" "B" "KB" "MB" "GB" "TB"]
1B = 8b
1KB = 1024B
1MB = 1024KB
1GB = 1024MB
1TB = 1024GB
[storage end]

[time: "s" "min" "h" "d"]
1min = 60s
1h = 60min
1d = 24h
[time end]

[length: "mm" "cm" "m" "km"]
1cm = 10mm
1m = 100cm
1km = 1000m
[length end]

[mass: "g" "on" "lb" "kg" "ston" "@" "q" "ton" ]
1on = 28.34g
1lb = 454g
1kg = 1000g
1ston = 14lb
1ston = 6356g
1@ = 11.34kg
1q = 100kg
1ton = 1000kg
[mass end]

[force: "dyn" "pdl" "ozf" "N" "lbf" "kN" ]
1dyn = 1e-05N
1pdl = 0.138255N
1ozf = 0.278014N
1lbf = 4.44822N
1kN = 1000N
[force end]